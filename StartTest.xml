<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="2">
<!-- 线程数（thread-count）需与config.properties中的设备个数对应 -->
	<listeners>
		<listener class-name="com.xiaoM.Report.utils.TestListener" />
		<listener class-name="com.xiaoM.Report.utils.TestReport" />
	</listeners>
	<test name="TestCase">
		<classes>
			<class name="com.xiaoM.BeginScript.BeginScript">
				<methods><!-- 执行指定的methods -->
					<exclude name="run2"/>
				</methods>
			</class>
		</classes>
	</test>
</suite> <!-- Suite -->